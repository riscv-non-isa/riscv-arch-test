fsgnjn.d_b1:
    config: 
      - check ISA:=regex(.*I.*D.*)
    mnemonics: 
      fsgnjn.d: 0
    rs1: 
      <<: *all_fregs
    rs2:
      <<: *all_fregs
    rd: 
      <<: *all_fregs
    op_comb: 
      <<: *rfmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b1(flen,64, "fsgnjn.d", 2)': 0
        
