fcvt.l.s_b1:
    config: 
      - check ISA:=regex(.*RV64.*I.*Zfinx.*)
    mnemonics: 
      fcvt.l.s: 0
    rs1: 
      <<: *all_regs
    rd: 
      <<: *all_regs
    val_comb:
      abstract_comb:
        'ibm_b1(flen,32, "fcvt.l.s", 1, True)': 0
        
