fmax.d_b1:
    config: 
      - check ISA:=regex(.*I.*Zfinx.*Zdinx.*)
    mnemonics: 
      fmax.d: 0
    rs1: 
      <<: *pair_regs
    rs2:
      <<: *pair_regs
    rd: 
      <<: *pair_regs
    op_comb: 
      <<: *rfmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b1(flen,64, "fmax.d", 2, True)': 0
        
fmax.d_b19:
    config: 
      - check ISA:=regex(.*I.*Zfinx.*Zdinx.*)
    mnemonics: 
      fmax.d: 0
    rs1: 
      <<: *pair_regs
    rs2:
      <<: *pair_regs
    rd: 
      <<: *pair_regs
    op_comb: 
      <<: *rfmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b19(flen,64, "fmax.d", 2, True)': 0
