fclass_b1:
    config: 
      - check ISA:=regex(.*I.*F.*)
    mnemonics: 
      fclass.s: 0
    rs1: 
      <<: *all_fregs
    rd: 
      <<: *all_regs
    val_comb:
      abstract_comb:
        'ibm_b1(flen,32, "fclass.s", 1)': 0
        
