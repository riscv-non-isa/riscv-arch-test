fsgnjn_b1:
    config: 
      - check ISA:=regex(.*I.*F.*)
    mnemonics: 
      fsgnjn.s: 0
    rs1: 
      <<: *all_fregs
    rs2:
      <<: *all_fregs
    rd: 
      <<: *all_fregs
    op_comb: 
      <<: *rfmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b1(flen,32, "fsgnjn.s", 2)': 0
        
