feq_b1:
    config: 
      - check ISA:=regex(.*I.*F.*)
    mnemonics: 
      feq.s: 0
    rs1: 
      <<: *all_fregs
    rs2:
      <<: *all_fregs
    rd: 
      <<: *all_regs
    op_comb: 
      <<: *sfmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b1(flen,32, "feq.s", 2)': 0
        
feq_b19:
    config: 
      - check ISA:=regex(.*I.*F.*)
    mnemonics: 
      feq.s: 0
    rs1: 
      <<: *all_fregs
    rs2:
      <<: *all_fregs
    rd: 
      <<: *all_regs
    op_comb: 
      <<: *sfmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b19(flen,32, "feq.s", 2)': 0
