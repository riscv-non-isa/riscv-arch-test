# Description: Makefrag for RV64IM architectural tests

rv64im_sc_tests = \
 add.uw-01 \
 andn-01 \
 bclr-01 \
 bclri-01 \
 bext-01 \
 bexti-01 \
 binv-01 \
 binvi-01 \
 bset-01 \
 bseti-01 \
 clmul-01 \
 clmulh-01 \
 clmulr-01 \
 clz-01 \
 clzw-01 \
 cpop-01 \
 cpopw-01 \
 ctz-01 \
 ctzw-01 \
 max-01 \
 maxu-01 \
 min-01 \
 minu-01 \
 orc.b-01 \
 orn-01 \
 rev8-01 \
 rol-01 \
 rolw-01 \
 ror-01 \
 rori-01 \
 roriw-01 \
 rorw-01 \
 sext.b-01 \
 sext.h-01 \
 sh1add-01 \
 sh1add.uw-01 \
 sh2add-01 \
 sh2add.uw-01 \
 sh3add-01 \
 sh3add.uw-01 \
 slli.uw-01 \
 xnor-01 \
 zext.h-01

rv64im_tests = $(addsuffix .elf, $(rv64im_sc_tests))

target_tests += $(rv64im_tests)
